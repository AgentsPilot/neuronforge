{
  "version": 2,
  "functions": {
    "app/api/run-scheduled-agents/route.ts": {
      "runtime": "nodejs20.x",
      "memory": 256,
      "maxDuration": 30
    },
    "app/api/run-agent/route.ts": {
      "runtime": "nodejs20.x", 
      "memory": 512,
      "maxDuration": 10
    }
  },
  "crons": [
    {
      "path": "/api/run-scheduled-agents",
      "schedule": "*/5 * * * *"
    }
  ]
}